{
  "name": "restaf-uidemos",
  "version": "2.2.0",
  "description": "UI demos for Viya using restaf",
  "scripts": {
    "implicit": "npx restaf-server --env=overridesBos.env --docker=Dockerfile",
    "start": "npx restaf-server --env=overridesBosc.env --docker=Dockerfile",
    "indocker": "npx restaf-server",
    "build": "rimraf public/shared/dist && webpack --config=webpack.config.js",
    "buildapp": "docker build -f Dockerfile -t viyaapp .",
    "runapp": "docker run -p 8080:8080  --env-file overrides.env  -t viyaapp"
  },
