{
  "name": "openai-assistant",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@monaco-editor/react": "^4.4.6",
    "@mui/icons-material": "^5.10.16",
    "@mui/material": "^5.10.16",
    "@sassoftware/restaf": "^5.2.4",
    "@sassoftware/restafedit": "^2.4.7",
    "@sassoftware/restaflib": "^5.2.4",
    "@sassoftware/viya-assistantjs": "^0.5.1",
    "@tinymce/tinymce-react": "^4.2.0",
    "ace-builds": "^1.13.1",
    "html-react-parser": "^5.1.8",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.4",
    "react-router-dom": "^6.4.4",
    "rehype-raw": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "testx": "react-scripts test",
    "eject": "react-scripts eject",
    "buildapp": "rimraf build && node createRoutes && cross-env REACT_APP_APPNAME=assistant react-scripts build",
    "app": "cross-env  APPLOC=./build APPENTRY=index.html node server --env=.env.production --docker=Dockerfile",
    "test": "cross-env  APPLOC=./component APPENTRY=index.html node server --env=.env.production --docker=Dockerfile",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.js'",
    "lint": "eslint . --ext .js --fix",
    "wpack": "webpack --env p=n library=ViyaApp --config=webpack.config.js && webpack --env p=y library=ViyaApp --config=webpack.config.js",
    "build4va": "rimraf dist && npm run wpack && rimraf ./component/dist && cp -r dist ./component/dist",
    "lib": "node server --env=.env.production --docker=Dockerfile",
    "indocker": "node server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@sassoftware/viya-appserverjs": "^2.16.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^2.0.6",
    "prettier": "^2.8.0",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "pretty-format": "^29.3.1",
    "react-scripts": "^5.0.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "webpack-cli": "^5.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
