		"url": "https://github.com/sassoftware/restaf-uidemos/issues"
	},
	"homepage": "https://github.com/sassoftware/restaf-uidemos/blob/viyaapi/README.md",
	"dependencies": {
		"@sassoftware/restaf": "next",
		"@sassoftware/restaflib": "next",
		"@sassoftware/viya-apiserverjs": "next",
