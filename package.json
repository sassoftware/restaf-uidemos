    "@sassoftware/restaflib": "^4.4.25",
    "@sassoftware/viya-appserverjs": "^2.0.2",
    "@sassoftware/viyaedit": "^0.11.1",
    "@tinymce/tinymce-react": "^3.12.2",
    "ace-builds": "^1.4.12",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.2",
    "deepcopy": "^2.1.0",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^1.0.5",
    "pretty-format": "^26.2.0",
    "react": "^17.0.2",
    "react-ace": "^9.1.3",
    "react-dom": "^17.0.2",
    "react-json-pretty": "^2.2.0",
    "react-markdown": "^8.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildapp": "node createRoutes && env-cmd --file ./.env react-scripts build",
    "dev": "node createRoutes.js && cross-env APPENTRY=/develop APPLOC=./public node server --env=.env --docker=Dockerfile",
    "newdev": "npx concurrently \"npm run app\" \"webpack-dev-server -d --open --content-base dist\"",
    "app": "cross-env  APPLOC=./build APPENTRY=index.html node server --env=.env --docker=Dockerfile",
    "compose": "docker-compose build && docker-compose up",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc.json --write '**/*.js'",
    "lint": "eslint . --ext .js --fix",
    "indocker": "node server.js",
    "jsdoc": "jsdoc --readme ./src/components/viyaedit/lib/README.md -c jsdoc.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
