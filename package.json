{
  "name": "restaf-uidemos",
  "version": "1.7.22",
  "description": "UI demos for Viya using restaf, restaflib and uilibrary",
  "main": "index.js",
  "scripts": {
    "start": "npx @sassoftware/viya-appserverjs --appenv=./appenv.js --env=./.env --docker=./Dockerfile",
    "indocker": "npx @sassoftware/viya-appserverjs",
    "dkrbuild": "docker build -f Dockerfile -t viyaapp .",
    "dkrrun": "docker run -p 8080:8080  --env-file .env  --name uidemos -t viyaapp",
    "dkrtest": "docker run -p 8081:8081 --env-file .env  --name uidemostest -v C:/Public/dev/reports:/mydata -t viyaapp"
  },
  "repository": "https://github.com/sassoftware/restaf/tree/gen2/packages/uidemos",
  "keywords": [],
  "dependencies": {
    "@sassoftware/viya-appserverjs": "dev"
  },
  "author": "Deva Kumar <deva.kumar@sas.com>",
  "license": "Apache-2.0"
}
